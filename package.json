{
  "name": "react-conditional-components",
  "version": "0.0.1",
  "description": "React library with conditional components to rendering",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test:w": "jest --watchAll",
    "test:v": "jest --verbose",
    "test:c": "jest --coverage",
    "test:d": "jest --debug",
    "test": "jest",
    "clean": "rm -Rf dist",
    "build": "npm run clean && tsc --skipLibCheck -p ."
